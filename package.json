    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-es2017": "7.0.0-beta.53",
    "@babel/preset-react": "7.0.0",
